{
  "name": "sou-h5-tempalte",
  "version": "2.2.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve --mode development --open",
    "test": "vue-cli-service build --mode test",
    "build": "vue-cli-service build --mode production --modern",
    "lint": "vue-cli-service lint",
